{
  "name": "interactive-comments-section",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "scripts": {
    "start:web": "npm run dev --workspace web",
    "start:server": "npm run dev --workspace server",
    "dev": "concurrently \"npm run start:web\" \"npm run start:server\""
  },
  "dependencies": {
    "concurrently": "~9.2.1",
    "zod": "^4.1.11"
  }
}
